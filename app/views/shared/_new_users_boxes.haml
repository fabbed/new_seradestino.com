#content_header
  #content_header_content
    =image_tag "title_new_users.png", :id => "title_new_users", :class => :fix, :style => "border: 0;"
    -if get_session && get_session.visitor.manipulation_level == "ugc_es"
      -Experiment.get_spanish_users.each do |user|
        =link_to_profile(user)
    -elsif get_session && get_session.visitor.manipulation_level == "ugc_mx"
      -Experiment.get_mexican_users.each do |user|
        =link_to_profile(user)
    -else
      -User.newest_first.with_avatar.active[0..6].each do |user|
        =link_to_profile(user)
